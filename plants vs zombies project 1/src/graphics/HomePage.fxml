<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.HomePage">
   <children>
      <ImageView fitHeight="600.0" fitWidth="600.0" layoutY="5.0">
         <image>
            <Image url="@../resources/images/backHomePage.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="350.0" fitWidth="350.0" layoutX="14.0" layoutY="100.0">
         <image>
            <Image url="@../resources/images/pngwing.com(1).png" />
         </image>
      </ImageView>
      <Button fx:id="saveE" layoutX="377.0" layoutY="190.0" mnemonicParsing="false" onAction="#saveEAction" prefHeight="42.0" prefWidth="190.0" text="save and quit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="Exit" layoutX="377.0" layoutY="284.0" mnemonicParsing="false" onAction="#exitAction" prefHeight="42.0" prefWidth="190.0" text="exit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="resume" layoutX="375.0" layoutY="377.0" mnemonicParsing="false" onAction="#resumeAction" prefHeight="42.0" prefWidth="190.0" text="resume">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
